String sessionToken = "dummy-session-token-identifier"; // taken from getSessionToken.do response
String orderId = "dummy-order-identifier"; // taken from openOrder.do response

SafechargeRequest getOrderDetailsRequest = new GetOrderDetailsRequest.Builder().addSessionToken(sessionToken)
                                                                               .addOrderId(orderId)
                                                                               .build();
GetOrderDetailsResponse getOrderDetailsResponse = (GetOrderDetailsResponse) requestExecutor.executeRequest(getOrderDetailsRequest);

System.out.println("Order status: " + getOrderDetailsResponse.getOrderStatus());
System.out.println("Transaction status: " + getOrderDetailsResponse.getTransactionStatus());